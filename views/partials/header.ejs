<header>
    <div class="jumbotron hidden-xs" style="background-image: url(<%-jumboPic%>)">
    </div>
    <nav class="navbar navbar-inverse sticky-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img src="<%=logo%>" height="50" />
                <div class="navbar-brand">Pictures by Coons</div>
            </div><!-- navbar-header -->
            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="/" <% if (pageID === 'home') { %> class="active" <% } %>  >Home</a></li>

                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">Genres
                        <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <% genres.forEach( genreVal => {  %>
                                <li ><a href="/<%=genreVal%>?page=1" <% if (pageID === genreVal) { %> class="active" <% } %>  ><%=genreVal%></a></li>
                            <% }) %>
                        </ul>
                    </li>
                                    
                    <li ><a href="/contact" <% if (pageID === 'contact') { %> class="active" <% } %>  >Contact</a></li>
                </ul><!-- nav -->
            </div><!-- navbar-collapse -->
        </div><!-- container -->
    </nav>
</header>
